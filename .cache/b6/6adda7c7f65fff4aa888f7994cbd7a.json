 uppercase;\r\n    animation: textfadein 2.2s;\r\n    animation-delay: 2.2s;\r\n    animation-fill-mode: forwards;\r\n}\r\n\r\n.title-second {\r\n    opacity: 0;\r\n    position: relative;\r\n    letter-spacing: 0.5em;\r\n    font-size: 3vw;\r\n    font-family: 'Playfair Display', serif;\r\n    font-style: italic;\r\n    animation: textfadein 3s;\r\n    animation-delay: 2.2s;\r\n    animation-fill-mode: forwards;\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n    .title-first {\r\n        font-size: 12vw;\r\n    }\r\n\r\n    .title-second {\r\n        font-size: 6vw;\r\n    }\r\n}\r\n\r\n#hero button {\r\n    transform: scaley(0);\r\n        opacity: 1;\r\n    /* grid-area: button; */\r\n    justify-self: center;\r\n    /* z-index: 22; */\r\n    margin: 20px;\r\n    animation: buttonfadein 2s;\r\n    animation-delay: 2.2s;\r\n    animation-fill-mode: forwards;\r\n    position: relative;\r\n    /* animation: name duration timing-function delay iteration-count direction fill-mode; */\r\n}\r\n\r\n\r\n\r\n#hero-image {\r\n    opacity: 0;\r\n        transform: scale(0.8);\r\n    width: 60vw;;\r\n    place-self: center;\r\n    \r\n    justify-self: center;\r\n    grid-area: text;\r\n    /* z-index: 1; */\r\n    animation: imagefadein 0.4s ease-in-out;\r\n    animation-delay: 2.3s;\r\n    animation-fill-mode: forwards;\r\n}\r\n\r\n#skills,\r\n#about {\r\n    /* padding: 20px; */\r\n    /* min-height: 100vh; */\r\n    max-height: 900px;\r\n    \r\n}\r\n\r\n#hero-items {\r\n    /* display: grid; */\r\n    grid-area: text;\r\n    text-align: center;\r\n    /* place-items: center; */\r\n    position: relative;\r\n    margin-top: auto;\r\n    margin-bottom: auto;\r\n    z-index: 3;\r\n    \r\n}\r\n\r\n@keyframes imagefadein {\r\n    0% {\r\n        opacity: 0;\r\n        transform: scale(0.8);\r\n    }\r\n\r\n   100% {\r\n       opacity: 0.15;\r\n    transform: scale(1);\r\n}\r\n}\r\n\r\n@keyframes namefadein {\r\n    0% {\r\n        opacity: 0;\r\n    }\r\n    50% {\r\n        opacity: 0;\r\n    }\r\n\r\n   100% {\r\n       opacity: 1;\r\n}\r\n}\r\n\r\n@keyframes textfadein {\r\n    0% {\r\n        \r\n        opacity: 0;\r\n        transform: scale(0.5);\r\n    }\r\n    40% {\r\n        \r\n        opacity: 0;\r\n        transform: scale(0.5);\r\n    }\r\n\r\n   100% {\r\n       opacity: 1;\r\n    transform: scale(1);\r\n}\r\n}\r\n\r\n@keyframes buttonfadein {\r\n \r\n    \r\n    0% {\r\n        /* border: 2px solid var(--text-color); */\r\n        transform: scaley(0);\r\n        opacity: 1;\r\n    }\r\n    \r\n    70% {\r\n        /* border: 2px solid var(--hover-color); */\r\n        transform: scaley(0);\r\n        opacity: 1;\r\n    }\r\n\r\n   100% {\r\n    /* border: 2px solid var(--text-color); */\r\n       opacity: 1;\r\n    transform: scale(1);\r\n}\r\n}\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"f78b31a64d8d98c9052117dada0da7c1","cacheData":{"env":{}}}