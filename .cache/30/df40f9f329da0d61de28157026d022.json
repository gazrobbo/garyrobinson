ources":{"CSS/main.css":":root {\r\n    --primary-color: #0f1014;\r\n    --overlay-color: #16181d;\r\n    --hover-color: #00a0fd;\r\n    --text-color: #ffffffee;\r\n    --vin-color: radial-gradient(circle at 45% 89%, rgba(15, 16, 20, 1), rgba(15, 16, 20, 0) 50%), radial-gradient(circle at 14% 10%, rgba(39, 66, 88, 0.42), rgba(97, 106, 141, 0) 50%), radial-gradient(circle at 57% 2%, #0f1014, #0f1014 50%), radial-gradient(circle at 85% 70%, rgba(15, 16, 20, 1), rgba(15, 16, 20, 0) 50%), radial-gradient(circle at 5% 90%, #0f1014, #0f1014 50%);\r\n    --menu-speed: 0.6s;\r\n}\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\nhtml {\r\n    scroll-behavior: smooth;\r\n}\r\n\r\nbody {\r\n    font-family: 'Hind', sans-serif;\r\n    background-color: var(--primary-color);\r\n    color: var(--text-color);\r\n}\r\n\r\nmain {\r\n\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    \r\n\r\n}\r\n\r\n#about,\r\n#skills,\r\n#contact {\r\n    scroll-snap-align: start;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\n.animation-container {\r\n    font-family: 'Montserrat', sans-serif;\r\n    font-weight: 700;\r\n}\r\n\r\n#about-text-panel h2,\r\n#skills h2,\r\n#contact-panel h2 {\r\n    font-size: 2rem;\r\n    padding-bottom: 50px;\r\n}\r\n\r\n@media only screen and (min-width: 600px) {\r\n\r\n    #about-text-panel h2,\r\n    #skills h2,\r\n    #contact-panel h2 {\r\n        font-size: 3rem;\r\n        padding-bottom: 40px;\r\n    }\r\n}\r\n\r\n.miain-title::after {\r\n    content: \"\";\r\n    height: 2px;\r\n    width: 70px;\r\n    background-color: var(--hover-color);\r\n    display: block;\r\n    margin-top: 5px;\r\n}\r\n\r\n/* .panel {\r\n    background-color: var(--overlay-color);\r\n    color: var(--text-color);\r\n    border-radius: 10px 10px 10px 10px;\r\n    padding: 50px 30px;\r\n} */\r\n\r\n/* .panel::before {\r\n    content: \"\";\r\n    height: 100%;\r\n    width: 100%;\r\n    background-color: white;\r\n} */\r\n\r\np {\r\n    opacity: 0.9;\r\n    line-height: 2rem;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n    transition: fill 0.3s;\r\n}\r\n\r\na:visited {\r\n    color: unset;\r\n    fill: unset;\r\n}\r\n\r\na:hover {\r\n    color: var(--hover-color);\r\n    fill: var(--hover-color);\r\n}\r\n\r\n.shape {\r\n    box-shadow: inset -2px -2px 6px rgba(0, 0, 0, 0.5), inset 2px 2px 6px rgba(255, 255, 255, 0.055), 3px 3px 3px rgba(0, 0, 0, 0.5);\r\n    background: linear-gradient(135deg, var(--overlay-color) 0%, #121318 100%);\r\n}\r\n\r\n.background {\r\n    background: var(--vin-color);\r\n    background-attachment: fixed;\r\n    scroll-snap-type: y mandatory;\r\n    /* overflow-y: scroll; */\r\n    /* test */\r\n}\r\n\r\n#skills,\r\n#about,\r\n#contact {\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    padding: 50px 15px;\r\n    max-width: 1000px;\r\n}\r\n\r\n@media only screen and (min-width: 600px) {\r\n\r\n    #skills,\r\n    #about,\r\n    #contact {\r\n        padding: 100px 90px;\r\n    }\r\n}\r\n\r\nbutton {\r\n    height: 60px;\r\n    width: 190px;\r\n    border: 2px solid var(--text-color);\r\n    background-color: transparent;\r\n    color: var(--text-color);\r\n    font-weight: 400;\r\n    text-transform: uppercase;\r\n    font-size: 13px;\r\n    letter-spacing: 0.174em;\r\n    transition: all 0.1s;\r\n}\r\n\r\nbutton:hover {\r\n    background-color: rgba(255, 255, 255, 0.082);\r\n    transform: scale(1.03);\r\n    border: 2px solid var(--hover-color);\r\n    color: var(--hover-color);\r\n}\r\n\r\n.buttonhover {\r\n    animation: buttonhover 1s ease-in-out;\r\n}\r\n\r\n@keyframes buttonhover {\r\n\r\n\r\n    0% {\r\n        border-bottom: 2px solid var(--hover-color);\r\n    }\r\n\r\n    33% {\r\n        border-right: 2px solid var(--hover-color);\r\n    }\r\n\r\n    33% {\r\n        border-top: 2px solid var(--hover-color);\r\n    }\r\n\r\n    100% {\r\n        border-left: 2px solid var(--hover-color);\r\n    }\r\n}"},"lineCount":183}},"error":null,"hash":"69a0d1785e77d0224977b39f